{
  "name": "cli-local",
  "version": "1.0.0",
  "description": "CLI for project scaffolding and code generation.",
  "main": "dist/main.js",
  "type": "commonjs",
  "bin": {
    "cl": "./dist/main.js"
  },
  "scripts": {
    "alias:gen": "tsx scripts/alias-sync-all.ts",
    "alias:watch": "chokidar \"src/core\" \"src/db\" \"src/features/*\" -i \"**/*.ts\" -c \"npm run alias:gen\"",
    "dev": "npm run alias:gen && concurrently \"npm run alias:watch\" \"tsx src/index.ts\"",
    "build": "tsc && tsc-alias && tsx scripts/copy-nuxt-templates.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "node dist/main.js",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "ejs": "^3.1.10",
    "execa": "^9.6.0",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.2.0",
    "module-alias": "^2.2.3",
    "tsconfig-paths": "^4.2.0",
    "zod": "^4.0.15"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@faker-js/faker": "^10.0.0",
    "@types/chalk": "^2.2.2",
    "@types/ejs": "^3.1.5",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.19.10",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "babel-jest": "^30.0.4",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.2.0",
    "eslint": "^9.30.1",
    "jest": "^29.7.0",
    "json5": "^2.2.3",
    "prettier": "^3.6.2",
    "ts-jest": "^29.1.1",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "_moduleAliases": {
    "@commands": "dist/commands",
    "@features": "dist/features",
    "@parsersMdj": "dist/features/parsersMdj",
    "@frameworks": "dist/frameworks",
    "@constants": "dist/constants",
    "@helpers": "dist/helpers",
    "@mocks": "dist/mocks",
    "@interfaces": "dist/interfaces",
    "@templates": "dist/templates",
    "@services": "dist/services",
    "@utils": "dist/utils"
  },
  "imports": {
    "@frameworks": "./src/features/frameworks",
    "@frameworks/*": "./src/features/frameworks/*",
    "@parsersMdj": "./src/features/parsersMdj",
    "@parsersMdj/*": "./src/features/parsersMdj/*",
    "@project": "./src/features/project",
    "@project/*": "./src/features/project/*",
    "@tools": "./src/features/tools",
    "@tools/*": "./src/features/tools/*",
    "@zod": "./src/features/zod",
    "@zod/*": "./src/features/zod/*"
  }
}
